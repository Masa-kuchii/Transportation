    "import copy\n",
    "import pandas\n",
    "import heapq\n",
    "import math\n",
    "from pandas import read_csv\n",
    "\n",
    "\"\"\"Network class\"\"\"\n",
    "class Node(object):\n",
    "    def __init__(self,nodeid,X,Y):\n",
    "        self.id = nodeid\n",
    "        self.X = X\n",
    "        self.Y = Y\n",
    "        return None\n",
    "            \n",
    "class Link(object):\n",
    "    def __init__(self,linkid,st,en,FFTT,para):\n",
    "        self.id = linkid\n",
    "        self.st = st\n",
    "        self.en = en\n",
    "        self.FFTT = FFTT\n",
    "        self.para = para\n",
    "        return None\n",
    "       \n",
    "class Network(object):\n",
    "    def __init__(self):\n",
    "        self.nodes = {}\n",
    "        self.links = {}\n",
    "        self.outlinks = {}\n",
    "        self.inlinks = {}\n",
    "        return None\n",
    "    \n",
    "    def addNode(self,node):\n",
    "        self.nodes[node.id] = node\n",
    "        return 0\n",
    "    \n",
    "    def addLink(self,link):\n",
    "        st = link.st\n",
    "        en = link.en\n",
    "        self.links[link.id] = link\n",
    "        if st not in self.outlinks:\n",
    "            self.outlinks[st]=[]\n",
    "        self.outlinks[st].append(link)\n",
    "        \n",
    "        if en not in self.inlinks:\n",
    "            self.inlinks[en]=[]\n",
    "        self.inlinks[en].append(link)\n",
    "        \n",
    "        return 0\n",
    "        \n",
    "        \n",
    "\"\"\"shortest path search using Dijkstra.　Return the list of links in the shortest path.\"\"\"    \n",
    "def Dijkstra(net, st, en, link_Tra_Volume):\n",
    "    nodes_Num = len(net.nodes)\n",
    "    previous_node = [-1]*nodes_Num\n",
    "    previous_links_id = [-1]*nodes_Num\n",
    "    links_id_inSP = []\n",
    "    node_cost = [1000000]*nodes_Num\n",
    "    node_cost[st]=0\n",
    "    heap = []\n",
    "    heapq.heappush(heap,(0,st))\n",
    "    while(len(heap) !=0):\n",
    "        current_node_cost, current_node_id = heapq.heappop(heap)\n",
    "        if (current_node_id == en):\n",
    "            break\n",
    "        for i in range(len(net.outlinks[current_node_id])):\n",
    "            cur_outlink = net.outlinks[current_node_id][i]\n",
    "            new_cost = current_node_cost + cur_outlink.FFTT + cur_outlink.para * link_Tra_Volume[cur_outlink.id]\n",
    "            next_node_id = cur_outlink.en\n",
    "            if (node_cost[next_node_id] > new_cost):\n",
    "                node_cost[next_node_id] = new_cost\n",
    "                heapq.heappush(heap,(new_cost,next_node_id))\n",
    "                previous_node[next_node_id]=current_node_id\n",
    "                previous_links_id[next_node_id] = cur_outlink.id\n",
    "    \n",
    "    previ_node = en\n",
    "    while (previ_node != -1): \n",
    "        links_id_inSP.append(previous_links_id[previ_node])\n",
    "        previ_node = previous_node[previ_node]\n",
    "    links_id_inSP.remove(-1)\n",
    "    \n",
    "    return links_id_inSP\n",
    "\"\"\"debug code for dijkstra\"\"\"\n",
    "#     print(\"shortest path is \", end=\"\")\n",
    "#     previ_node = en\n",
    "#     while (previ_node != -1):\n",
    "#         if (previ_node != -1):\n",
    "#             print(str(previ_node)+\">\", end=\"\")\n",
    "#         else:\n",
    "#             print(str(previ_node))    \n",
    "#         links_id_inSP.append(previous_links_id[previ_node])\n",
    "#         previ_node = previous_node[previ_node]\n",
    "    \n",
    "#     links_id_inSP.remove(-1)\n",
    "#     print(\"\")\n",
    "#     print(\"shortest path cost is \" + str(node_cost[en]))\n",
    "    \n",
    "#     return links_id_inSP\n",
    "\n",
    "\"\"\"objective function Z\"\"\"\n",
    "def Z_function(network, link_size, alpha, Xnn1, yy):\n",
    "    z = 0\n",
    "    for i in range(link_size):\n",
    "        z += (network.links[i].FFTT)*(alpha*Xnn1[i]+(1-alpha)*yy[i]) + ((network.links[i].para)/2)*(alpha*Xnn1[i]+(1-alpha)*yy[i])*(alpha*Xnn1[i]+(1-alpha)*yy[i])\n",
    "    return z\n",
    "\n",
    "\"\"\"UE using Frank-Wolfe algorithm\"\"\"\n",
    "def assignment(network,orig, dest, OD_vol):\n",
    "    GR = 1.6180339887\n",
    "    THRES_DELTA = 0.01\n",
    "    THRES_XI = 0.0001\n",
    "    DIFF = 100000\n",
    "    link_size = len(network.links)\n",
    "    \n",
    "#Initialize Xn, Xn2 and y. (Xn = Xn2, Xn-1 = Xn1, y = y)\n",
    "    Xn1 = [0]*link_size\n",
    "    Xn2 = [0]*link_size\n",
    "    y = [0]*link_size\n",
    "    \n",
    "#All-or-nothing assignment by free flow link travel times.\n",
    "    links_id_inSP = Dijkstra(network, orig, dest, Xn1)\n",
    "    for linID in links_id_inSP:\n",
    "        Xn1[linID] = OD_vol\n",
    "#iterate until the difference becomes less than threshold.\n",
    "    n = 1\n",
    "    while (DIFF > THRES_DELTA):\n",
    "# calculte the slope.\n",
    "        for i in range(link_size):\n",
    "            y[i] = 0\n",
    "        links_id_inSP = Dijkstra(network, orig, dest, Xn1)\n",
    "        for linID in links_id_inSP:\n",
    "            y[linID] = OD_vol\n",
    "        \n",
    "#Find alpha using the golden section search.\n",
    "        minz = 100000\n",
    "        minalpha = 0.0\n",
    "        if (Z_function(network, link_size, 0.0, Xn1, y) < Z_function(network, link_size, 1.0, Xn1, y)):\n",
    "            alpha1 = 1.0\n",
    "            alpha2 = 0.0\n",
    "        else: \n",
    "            alpha1 = 0.0\n",
    "            alpha2 = 1.0\n",
    "        \n",
    "        while (math.fabs(alpha1-alpha2) > THRES_XI):\n",
    "            alpha3 = (alpha1 * 1.0 + alpha2 * GR)/(1.0+GR)\n",
    "            if(Z_function(network, link_size, alpha2, Xn1, y) < Z_function(network, link_size, alpha3, Xn1, y)):\n",
    "                alpha1 = copy.deepcopy(alpha3)\n",
    "            else:\n",
    "                alpha1 = copy.deepcopy(alpha2)\n",
    "                alpha2 = copy.deepcopy(alpha3)\n",
    "\n",
    "        minalpha = alpha2\n",
    "        minz = Z_function(network, link_size, alpha2, Xn1, y)\n",
    "        \n",
    "#       update link traffic volume using minalpha. \n",
    "        DIFF = 0.0\n",
    "        for i in range(link_size):\n",
    "            Xn2[i] = (minalpha)*Xn1[i] + (1-minalpha)*y[i]\n",
    "            DIFF += abs(Xn2[i]-Xn1[i])\n",
    "        Xn1 = copy.deepcopy(Xn2)\n",
    "        print(\"{}th\".format(n))\n",
    "        n +=1\n",
    "        print(\"Xn1=\",Xn1)\n",
    "    print(\"\")\n",
    "    print(\"Link traffic volume in UE is\")\n",
    "    print(Xn2)"
   ]
